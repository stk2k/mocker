{
  "name": "stk2k/mocker",
  "description": "Simple mock/stub library",
  "keywords": ["PHP","PHP7","PHP8","library","mock","stub","test double"],
  "homepage": "https://github.com/stk2k/mocker",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Katsuki Shuto",
      "email": "stk2k@sazysoft.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "stk2k/file-system": "~0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.15",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "autoload": {
    "psr-0": {
      "Foo\\Bar\\": "include/"
    },
    "psr-4": {
      "stk2k\\mocker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "stk2k\\mocker\\test\\": "test/"
    }
  }
}